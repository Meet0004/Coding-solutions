class Solution {
    public boolean isPalindrome(int x) {
        String str = Integer.toString(x);
        int n = str.length();

        for (int i=0; i<n/2; i++) {
            int j = n - 1 - i;
            if (str.charAt(i) != str.charAt(j)) {
                return false;
            }
        }
        return true;
    }
}
